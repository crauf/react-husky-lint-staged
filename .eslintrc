{
    "extends": [
      "wesbos"
    ],
    "rules": {
      "no-console": 1,
      "prettier/prettier": [
        1,
        {
          "trailingComma": "es5",
          "singleQuote": true,
          "printWidth": 120,
          "tabWidth": 4,
          "arrowParens": "always"
        }
      ],
      "react/jsx-props-no-spreading": 0,
      "camelcase": 1,
      "no-shadow": 1,
      "no-nested-ternary": 1,
      "react-hooks/rules-of-hooks": 1,
      "jsx-a11y/anchor-has-content": 1,
      "global-require": 0,
      "react/jsx-no-duplicate-props": 1,
      "no-plusplus": 1,
      "no-return-await": 1,
      "import/no-unresolved": 0
    }
  }